SUPER KEYWORD (INHERITANCE):


import java.util.*;

class Parent{
    int a=100,b=200;
}
class Child extends Parent{
    int a=18,b=78;
    void show(int a,int b){
    System.out.println(a+b);
    System.out.println(super.a+super.b);
     System.out.println(this.a+this.b);
    
}
}
class Test {
    public static void main(String[] args) {
        Child c1=new Child();
        c1.show(45,67);
    }
}
